        <rdfs:isDefinedBy rdf:resource="http://www.ics.forth.gr/isl/MarineTLO/v3/marinetlo.owl"/>
    </owl:Class>
    


    <!-- http://www.ics.forth.gr/isl/MarineTLO/v3/marinetlo.owl#D10SoftwareExecution -->

