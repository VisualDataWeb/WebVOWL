  <rdfs:range rdf:resource="http://rdfs.org/sioc/ns#Space"/>
  <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#has_subscriber">
