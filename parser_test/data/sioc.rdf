  <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
</owl:DatatypeProperty>

<owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#owner_of">
  <rdfs:label xml:lang="en">owner of</rdfs:label>
