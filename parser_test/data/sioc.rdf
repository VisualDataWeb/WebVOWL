<owl:ObjectProperty rdf:about="http://rdfs.org/sioc/ns#space_of">
  <rdfs:label xml:lang="en">space of</rdfs:label>
  <owl:inverseOf rdf:resource="http://rdfs.org/sioc/ns#has_space"/>
