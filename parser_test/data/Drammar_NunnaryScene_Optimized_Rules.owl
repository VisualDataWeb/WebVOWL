    <!-- http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#Timeline -->

    <owl:Class rdf:about="&drammar;Timeline">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&drammar;DramaList"/>
                    <owl:Restriction>
