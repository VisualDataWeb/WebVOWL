        <rdfs:subPropertyOf rdf:resource="&drammar;inverseObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#isDoneStateOf -->

    <owl:ObjectProperty rdf:about="&drammar;isDoneStateOf">
